# Display time
SPACESHIP_TIME_SHOW=false

# Make the prompt span across two lines
SPACESHIP_PROMPT_SEPARATE_LINE=false

# Adds a newline character before each prompt line
SPACESHIP_PROMPT_ADD_NEWLINE=false

# Language Symbols
SPACESHIP_GOLANG_SYMBOL="󰟓 "
SPACESHIP_RUBY_SYMBOL=" "

# Do not show docker version
SPACESHIP_DOCKER_SHOW=false
SPACESHIP_DOCKER_COLOR="blue"

# Number of folders of cwd to show in prompt, 0 to show all
SPACESHIP_DIR_TRUNC=2

# Do not show git section's prefix
SPACESHIP_GIT_PREFIX=""

# Do not show git status
SPACESHIP_GIT_STATUS_SHOW=false

# Do not show AWS details
SPACESHIP_AWS_SHOW=false
